{
  "manifest_version": 3,
  "name": "Nothing UI NewTab",
  "version": "1.0",
  "host_permissions": [
    "https://fonts.googleapis.com/",
    "https://fonts.gstatic.com/"
  ],
  "web_accessible_resources": [
    {
      "resources": ["*.ttf", "*.woff", "*.woff2", "*.ttf", "*.eot"],
      "matches": ["<all_urls>"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs", "scripting"]
}
